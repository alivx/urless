pipeline {
  agent {label "appBuilder"}
  environment{
    appName = """${sh(returnStdout:true,script:'date')} """.trim()
    password = credentials('alid')
  }
  parameters{
    string(name:'Hello',defaultValue:'Hello0',description:"THis is hellow")
  }
  stages{
    stage("Pull changes"){
      environment{
        buildStep = "True"
      }
      steps{
        sh 'printenv'
        sh 'echo $password > ali'
      }
    }
    stage("Artifact"){
      steps{
        archiveArtifacts artifacts: "ali",fingerprint:true
        echo "${params.Greeting} World!"
      }
    }
  }
}